{
  "gitbook": "3.2.2",
  "title": "gitbook-notes",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "gitbook-notes": "https://github.com/atlantis1024/gitbook-notes"
    }
  },
  "plugins": [
    "-fontsettings",
    "-highlight",
    "-livereload",
    "-lunr",
    "-search",
    "anchor-navigation-ex",
    "anchors",
    "disqus",
    "edit-link",
    "emphasize",
    "expandable-chapters-small",
    "github",
    "github-buttons",
    "prism",
    "search-plus",
    "sectionx",
    "simple-page-toc",
    "splitter",
    "tbfed-pagefooter",
    "theme-comscore",
    "toggle-chapters"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "isRewritePageTitle": false,
      "float": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      },
      "pageTop": {
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "disqus": {
      "shortName": "gitbook-notes"
    },
    "edit-link": {
      "base": "https://github.com/atlantis1024/gitbook-notes/blob/master",
      "label": "编辑此页面"
    },
    "fontsettings": {
      "theme": "white",
      "famliy": "serif",
      "size": 2
    },
    "github": {
      "url": "https://github.com/atlantis1024/gitbook-notes"
    },
    "sectionx": {
      "tag": "b"
    },
    "simple-page-toc": {
      "maxDepth": 4,
      "skipFirstH1": true
    },
    "theme-default": {
      "showLevel": true
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © victor zhang 2017",
      "modify_label": "该文件上次修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}
